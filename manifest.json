{
    "manifest_version": 2,
    "version": "0.9.0.4",
    "name": "firefox jquery ajax sample",
    "description": "Demonstration of Ajax call failing in Firefox",

    "icons": {
	"16": "bug.png"
    },
    "browser_action":
    {
	"default_icon": "bug.png"
    },
        "permissions": [
		"https://www.example.com/*", "http://localhost/", "http://localhost:8001/*", "http://www.example.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
		"https://www.example.com/*", "http://localhost/", "http://localhost:8001/*", "http://www.example.com/*"
            ],
            "js": [
		"jquery-3.6.0.min.js",
		"sample.js"
            ]
	}
    ]
}




